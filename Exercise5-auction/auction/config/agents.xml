<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### AUCTION AGENTS ### -->
	<agent name="auction-main-01">
		<set class-path="bin/"/>
		<set class-name="bestagent.AuctionAgent"/>
		<set algorithm="AUCTION"/>
	</agent>

	<agent name="auction-risky">
		<set class-path="bin/"/>
		<set class-name="bestagent.AuctionAgent"/>
		<set algorithm="AUCTION"/>
		<set strategy="RISKY"/>
		<set distribution="FALSE"/>
	</agent>
	
	<agent name="auction-safe">
		<set class-path="bin/"/>
		<set class-name="bestagent.AuctionAgent"/>
		<set algorithm="AUCTION"/>
		<set strategy="SAFE"/>
		<set distribution="FALSE"/>
	</agent>

	<agent name="auction-catch">
		<set class-path="bin/"/>
		<set class-name="bestagent.AuctionAgent"/>
		<set algorithm="AUCTION"/>
		<set strategy="CATCHING_UP"/>
		<set distribution="FALSE"/>
	</agent>

	<agent name="auction-best">
		<set class-path="bin/"/>
		<set class-name="bestagent.AuctionAgent"/>
		<set algorithm="AUCTION"/>
		<set strategy="VARIABLE"/>
		<set distribution="true"/>
	</agent>



	<agent name="auction-honnest">
		<set class-path="bin/"/>
		<set class-name="bestagent.AuctionAgent"/>
		<set algorithm="AUCTION"/>
		<set strategy="HONNEST"/>
		<set distribution="FALSE"/>
	</agent>
	
</agents>
