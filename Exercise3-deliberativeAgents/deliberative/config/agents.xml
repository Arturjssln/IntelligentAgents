<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### DELIBERATIVE AGENTS ### -->
	<agent name="deliberative-naive">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeAgent"/>
		<set algorithm="NAIVE"/>
		<set heuristic="NONE"/>
	</agent>

	<agent name="deliberative-bfs">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeAgent"/>
		<set algorithm="BFS"/>
		<set heuristic="NONE"/>
	</agent>

	<agent name="deliberative-astar">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeAgent"/>
		<set algorithm="ASTAR"/>
		<set heuristic="NONE"/>
	</agent>

	<agent name="deliberative-main">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeAgent"/>
		<set algorithm="ASTAR"/>
		<set heuristic="LONGESTTASK"/>
	</agent>

	
</agents>
