<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### CENTRALIZED AGENTS ### -->
	<agent name="centralized-random">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedAgent"/>
		<set algorithm="NAIVE"/>
	</agent>

	<agent name="centralized-seq">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedAgent"/>
		<set algorithm="SLS"/>
		<set initialization="SEQUENTIAL"/>
	</agent>

	<agent name="centralized-dis">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedAgent"/>
		<set algorithm="SLS"/>
		<set initialization="DISTRIBUTED"/>
	</agent>

	<agent name="centralized-slave">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedAgent"/>
		<set algorithm="SLS"/>
		<set initialization="SLAVE"/>
	</agent>

	<agent name="centralized-main">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedAgent"/>
		<set algorithm="SLS"/>
		<set initialization="DISTRIBUTED"/>
	</agent>
	
</agents>
